<%- include('./partials/header') %>

<table>
  <thead>
    <tr>
      <th>Airline</th>
      <th>Airport</th>
      <th>flightNum</th>
      <th>Departure</th>
    </tr>
  </thead>
  <tbody>
    <% flights.forEach(function(flight) {%> 
        <tr>
          <td><%= flight.airline%></td>
          <td><%= flight.airport%></td>
        </tr>
      <% }) %>
  </tbody>
</table>


<%- include('./partials/footer') %>